{
  "name": "@alicloud/iconfont-helper",
  "version": "1.1.33",
  "description": "iconfont.cn 辅助，用于注入 iconfont 或 webfont 并返回对应的 font 名，是写组件的好帮手",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/cjs/index.js",
  "module": "build/es/index.js",
  "types": "build/types/index.d.ts",
  "homepage": "https://github.com/aliyun/alibabacloud-console-base/tree/master/packages/iconfont-helper",
  "author": {
    "name": "Jianchun Wang",
    "email": "justnewbee@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliyun/alibabacloud-console-base.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "iconfont",
    "helper"
  ],
  "devDependencies": {
    "@alicloud/console-toolkit-cli": "^1.2.13",
    "@alicloud/console-toolkit-preset-component": "^1.2.29",
    "@types/react": "^17.0.24",
    "react": "^17.0.2",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "styled-components": ">=5"
  },
  "scripts": {
    "start": "breezr start-storybook",
    "test": "breezr test:unit",
    "build:esm": "breezr build --engine babel --es-module",
    "build:cjs": "breezr build --engine babel",
    "build:bundle": "breezr build --engine webpack",
    "build:typings": "tsc --outDir build/types --declaration --emitDeclarationOnly",
    "build": "yarn build:esm && yarn build:cjs && yarn build:typings",
    "clean": "rm -rf build",
    "prepublishOnly": "yarn clean && yarn build"
  }
}
